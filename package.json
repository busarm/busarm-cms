        "@types/cookie-session": "^2.0.44",
        "@types/cors": "^2.8.12",
        "@types/crypto-js": "^4.1.1",
        "@types/express": "^4.17.13",
        "@types/express-session": "^1.17.4",
        "@types/flat": "^5.0.2",
        "@types/node": "^17.0.38",
