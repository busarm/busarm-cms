{
    "name": "busarm-cms",
    "version": "1.0.0",
    "description": "Busarm CMS",
    "private": "true",
    "main": "server.js",
    "scripts": {
        "build": "npx tsc",
        "watch": "npx tsc --watch",
        "sync": "npx ts-node ./scripts/sync.ts",
        "test": "echo \"Error: no test specified\" && exit 1",
        "start-dev": "npm run sync && concurrently \"npm run watch\" \"nodemon -e ts,tsx,js,jsx,ejs,ts,json,html,css -i dist/** -i node_modules/** -i .adminjs/** server.ts\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Busarm/busarm-cms.git"
    },
    "keywords": [
        "cms",
        "bus",
        "busarm"
    ],
    "author": "Samuel Gabriel",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Busarm/busarm-cms/issues"
    },
    "homepage": "https://github.com/Busarm/busarm-cms#readme",
    "dependencies": {
        "@adminjs/design-system": "^2.2.4",
        "@adminjs/express": "^4.1.3",
        "@adminjs/sequelize": "^2.1.5",
        "adminjs": "^5.10.3",
        "compression": "^1.7.4",
        "connect-session-sequelize": "^7.1.4",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "crypto-js": "^4.1.1",
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "express-formidable": "^1.2.0",
        "express-session": "^1.17.3",
        "flat": "^4.1.1",
