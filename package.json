{
  "name": "busarm-cms",
  "version": "1.0.0",
  "description": "Busarm CMS",
  "private": "true",
  "main": "server.js",
  "scripts": {
    "build": "npx tsc",
    "watch": "npx tsc --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "sync:db": "npx ts-node \"./scripts/sync_db.ts\" ",
    "sync:env": "npx ts-node \"./scripts/sync_env.ts\" ",
    "start:dev": "concurrently \"npm run watch\" \"nodemon -e ts,tsx,js,jsx,ejs,ts,json,html,css -i dist/** -i node_modules/** -i .adminjs/** server.ts\"",
    "start": "npm run sync:env && npm run sync:db && npm run start:dev",
    "sync:prod:db": "node \"./dist/scripts/sync_db.js\" ",
    "sync:prod:env": "node \"./dist/scripts/sync_env.js\" ",
    "stop:prod": "pm2 stop cms-app 2> /dev/null",
    "start:prod": "pm2 start \"./dist/server.js\" --name cms-app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Busarm/busarm-cms.git"
  },
  "dependencies": {
    "@adminjs/design-system": "^3.0.0",
    "@adminjs/express": "^5.0.0",
    "@adminjs/sequelize": "^3.0.0",
    "@types/react": "^18.0.16",
    "@types/styled-components": "^5.1.25",
    "adminjs": "^6.0.6",
    "app-root-path": "^3.1.0",
    "aws-sdk": "^2.1199.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "express-useragent": "^1.0.15",
    "fast-geoip": "^1.1.88",
    "flat": "^5.0.2",
    "helmet": "^5.1.1",
    "minimist": "^1.2.6",
    "mysql2": "^2.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-view": "^1.21.3",
    "react-simple-image-viewer": "^1.2.2",
    "sequelize": "^6.21.3",
    "sequelize-auto": "^0.8.8",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/express-useragent": "^1.0.2",
    "@types/flat": "^5.0.2",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.6.4",
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
